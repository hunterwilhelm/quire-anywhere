{
  "name": "Quire Anywhere",
  "version": "0.1.5",
  "version_name": "Beta 0.1.5",
  "description": "The Best Chrome Extension For Quire!",
  "permissions": [
    "contextMenus",
    "storage",
    "https://quire.io/*",
    "https://zicy.net/*",
    "http://zicy.net/*",
    "https://cdnjs.cloudflare.com/*"
  ],
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_title": "Quire Anywhere",
    "default_popup": "/views/popup/popup.html",
    "default_icon": {
      "16": "images/quire-anywhere-16.png",
      "32": "images/quire-anywhere-32.png",
      "48": "images/quire-anywhere-48.png",
      "128": "images/quire-anywhere-128.png"
    }
  },
  "icons": {
    "16": "images/quire-anywhere-16.png",
    "32": "images/quire-anywhere-32.png",
    "48": "images/quire-anywhere-48.png",
    "128": "images/quire-anywhere-128.png"
  },
  "manifest_version": 2
}
